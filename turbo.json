{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "generate": {},
    "build": {
      "dependsOn": [
        "generate",
        "^build"
      ],
      "outputs": [
        ".next/**",
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "generate"
      ]
    },
    "test": {},
    "lint": {}
  }
}
